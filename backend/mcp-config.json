      "args": ["@akiojin/openai-mcp-server"],
      "env": {
        "OPENAI_API_KEY": "${OPENAI_API_KEY}"
      }
    },
    "openai-alternative": {
      "command": "npx",
      "args": ["@mzxrai/mcp-openai"],
